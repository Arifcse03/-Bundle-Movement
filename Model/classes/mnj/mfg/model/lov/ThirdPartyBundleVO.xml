  <SQLQuery>
    <![CDATA[SELECT 
       MMCS.BANDEL_NO bundle_no,
       MMCS.SIZ_BND_ID size_value,
       MMCS.SRQ_NUMBER_FRM from_piece_no,
       MMCS.SRQ_NUMBER_TO to_piece_no,
       MMCS.SHADE_ID shade,
       MMCS.SRQ_NUMBER_TO - MMCS.SRQ_NUMBER_FRM + 1 total_qty,
       MMCL.ATTRIBUTE1 cut_no,
       MMC.CUTISSUE_NO
  FROM MNJ_MFG_CUTISSUANCE        MMC,
       MNJ_MFG_CUTISSUANCE_LINE   MMCL,
       MNJ_MFG_CUTISSUANCE_SZLINE MMCS
 WHERE MMCS.CUTISSUE_LINE_ID = MMCL.CUTISSUE_LINE_ID
   AND MMCL.CUTISSUE_ID      = MMC.CUTISSUE_ID
   AND MMC.ISSUE_TO          = 'Issued to Third Party']]>
  </SQLQuery>
