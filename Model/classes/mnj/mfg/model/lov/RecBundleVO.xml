  <SQLQuery>
    <![CDATA[SELECT distinct dd.size_id detail_id, dd.bandel_no bundle_no,
       dd.iss_quantity size_value, dd.srq_number_frm from_piece_no,
       dd.srq_number_to to_piece_no, dd.shade_id shade,
       dd.srq_number_to - dd.srq_number_frm + 1 total_qty,
       ll.attribute1 cut_no, hhh.cutissue_no ISSUE_DOC
  FROM mnj_mfg_cutissuance_szline dd,
       mnj_mfg_cutissuance_line ll,
       mnj_mfg_cutissuance hhh
 WHERE ll.cutissue_line_id = dd.cutissue_line_id
   AND ll.cutissue_id = hhh.cutissue_id
   and hhh.ISSUE_TO like 'Issue%'
   order by dd.bandel_no]]>
  </SQLQuery>
